{% import 'macros/base.twig' as macros %}
{% extends 'layouts/mainLayout.twig' %}

{% set logo = 'img/logo.png' %}
{% set title = videoTitle %}
{% set description = videoSubtitle %}
{% set image = logo %}
{% set url = 'url' %}

{% block meta %}
{{ macros.meta(title, description, image, url) }}
{% endblock %}

{% block css %}
{{ macros.css("css/style.min.css") }}
{% endblock %}

{% block js %}
{{ macros.js("js/jquery.min.js") }}
{{ macros.js("js/jquery.lazy.min.js") }}
{{ macros.js("js/jquery.main.js") }}
{% endblock %}

{% block nav %}

{% endblock %}

{% block content %}
{%
    include 'pages/webinar-video/section-video.twig' with {
        title: videoTitle,
        subtitle: videoSubtitle,
        video: videoURL
    }
%}
{% endblock %}

{% block footer %}

{% endblock %}